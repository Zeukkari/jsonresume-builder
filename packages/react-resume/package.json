{
  "name": "@resumebuilder/react-resume",
  "private": true,
  "version": "0.9.0",
  "author": {
    "name": "Timo Aho",
    "email": "timo.aho@pm.me"
  },
  "description": "React resume component",
  "repository": "git@github.com:Zeukkari/react-resume.git",
  "license": "MIT",
  "main": "./dist/react-resume.cjs.js",
  "module": "./dist/react-resume.esm.js",
  "browser": "./dist/react-resume.js",
  "scripts": {
    "build": "npm run build:storybook && npm run build:components",
    "build:components": "rollup -c",
    "build:storybook": "build-storybook -c .storybook -o dist/storybook",
    "check": "npm run lint && npm test",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "start": "npm run start:dev",
    "start:dev": "rollup -c -w",
    "start:storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest src",
    "test:coverage": "jest src --coverage && codecov"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "grommet-controls": "^1.0.11",
    "grommet-theme-aruba": "^0.1.0",
    "grommet-theme-dxc": "^0.1.0",
    "grommet-theme-hp": "^0.1.0",
    "grommet-theme-hpe": "^0.4.1",
    "grommet-theme-v1": "^0.1.0",
    "moment": "^2.24.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "resume-schema": "github:Zeukkari/resume-schema#zeukkari/update-deps",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "grommet": "^2.2.1",
    "grommet-icons": "^4.1.0",
    "grommet-controls": "^1.0.10",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^4.1.11",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "codecov": "^3.1.0",
    "eslint": "^5.13.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "grommet": "^2.3.1",
    "grommet-icons": "^4.1.0",
    "jest": "^24.0.0",
    "react-dev-utils": "^7.0.1",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/js/components/**/*.{js}",
      "src/js/theme/**/*.{js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|templates|node_modules)[/\\\\]"
    ]
  },
  "pre-commit": [
    "check"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
