  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run --stream build",
    "check": "lerna run --stream check",
    "clean": "lerna run --stream clean",
    "install": "lerna link --force-local && npm run bootstrap",
    "lint": "lerna run --stream lint",
    "lint:fix": "lerna run --stream lint:fix",
    "storybook:resume": "lerna exec --stream --scope @resumebuilder/react-resume -- npm run start:storybook",
    "storybook": "lerna exec --stream --scope @resumebuilder/live-editor -- npm run start:storybook",
    "test": "lerna run --stream test",
    "test:coverage": "lerna run --stream test:coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.1.0",
    "eslint": "^5.13.0",
    "eslint-plugin-react": "^7.12.4",
    "lerna": "^3.10.8"
  },
  "dependencies": {}
