{
  "name": "jsonresume-builder",
  "version": "0.1.0",
  "author": "Timo Aho",
  "description": "JSON Resume Builder",
  "repository": "git@github.com:Zeukkari/jsonresume-builder.git",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:storybook": "build-storybook -c .storybook -o dist/storybook",
    "check": "npm run lint && npm test",
    "lint": "eslint src",
    "start": "webpack-dev-server --mode development",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "test:coverage": "jest --coverage && codecov"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "ajv": "^6.7.0",
    "brace": "^0.11.1",
    "grommet": "^2.3.1",
    "grommet-icons": "^3.2.1",
    "jsoneditor": "^5.27.1",
    "jsoneditor-react": "^1.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-json-editor-ajrm": "^2.5.9",
    "resume-schema": "github:Zeukkari/resume-schema#zeukkari/update-deps",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@storybook/react": "^4.1.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-grommet": "^0.5.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.2",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-parallel": "^0.3.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "jest": "^23.6.0",
    "jest-styled-components": "^5.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "pre-commit": "^1.2.2",
    "react-dev-utils": "^5.0.0",
    "react-test-renderer": "^16.7.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1",
    "z-schema": "^3.24.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/templates/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|templates|node_modules)[/\\\\]"
    ]
  },
  "pre-commit": [
    "check"
  ]
}
